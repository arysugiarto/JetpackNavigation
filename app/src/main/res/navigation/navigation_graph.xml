<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/firstFragment">

    <fragment
        android:id="@+id/firstFragment"
        android:name="arysugiarto10.id.jetpacknavigation.FirstFragment"
        android:label="fragment_first"
        tools:layout="@layout/fragment_first" >
        <action
            android:id="@+id/action_firstFragment_to_seconFragment"
            app:destination="@id/seconFragment" />
    </fragment>
    <fragment
        android:id="@+id/seconFragment"
        android:name="arysugiarto10.id.jetpacknavigation.SeconFragment"
        android:label="fragment_secon"
        tools:layout="@layout/fragment_secon" />
</navigation>